<project>
  <target name="DeployPortal">
  	<echo>Move all Portal jar files to CATALINA_HOME.</echo>
  	<property environment="env"/>
  	<copy todir="${env.CATALINA_HOME}/lib" overwrite="true">
  	    <fileset dir="${env.CATALINA_HOME}/webapps/portal/WEB-INF/lib">
  	        <include name="*.jar"/>
  	    </fileset>
  	</copy>
  	<delete dir="${env.CATALINA_HOME}/webapps/portal/WEB-INF/lib"/>
  </target>
</project>