      <distribution>repo</distribution>
    </license>
  </licenses>
  <organization>
    <name>Cyberelay</name>
    <url>http://www.cyberelay.org/</url>
  </organization>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.5</source>
          <target>1.5</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <modules>
