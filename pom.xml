    <module>legacy-portlet-container-impl</module>
    <module>portlet-descriptor</module>
    <module>util</module>
  </modules>
  <dependencies>
    <dependency>
      <groupId>javax.servlet</groupId>
      <artifactId>jsp-api</artifactId>
      <version>2.0</version>
