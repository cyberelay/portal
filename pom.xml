  <groupId>org.cyberelay.portal</groupId>
  <artifactId>cyberelay-portal</artifactId>
  <packaging>pom</packaging>
  <name>Cyberelay Portal</name>
  <version>1.0.0</version>
  <description>Cyberelay Portal is an portal server with JSR168/JSR286 support.</description>
  <url>http://www.cyberelay.org/portal</url>
  <issueManagement>
    <system>jira</system>
    <url>http://null</url>
